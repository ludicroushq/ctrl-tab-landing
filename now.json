{
  "name": "ludicrous-tab-landing",
  "alias": "tab.ludicrous.xyz",
  "type": "static",
  "static": {
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  },
  "engines": {
    "node": "^8.0.0"
  },
  "env": {
    "BASE_URL": "https://tab.ludicrous.xyz"
  }
}
